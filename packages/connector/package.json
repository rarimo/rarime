{
  "name": "@rarimo/rarime-connector",
  "version": "2.1.0-rc.3",
  "description": "Facilitates interaction between a DApp and RariMe MetaMask snap",
  "repository": {
    "type": "git",
    "url": "https://github.com/rarimo/rarime.git"
  },
  "license": "(MIT-0 OR Apache-2.0)",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "yarn run prebuild && yarn run build:lib && yarn run build:types && yarn run build:alias",
    "build:lib": "tsc --build tsconfig.json",
    "build:types": "tsc --emitDeclarationOnly",
    "build:alias": "tsc-alias",
    "lint": "yarn run lint:style && yarn run lint:types",
    "lint:types": "tsc --noEmit --pretty",
    "lint:style": "eslint --color 'src/**/*.{js,ts,tsx}'",
    "lint:style:fix": "yarn run lint:style --fix",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@cosmjs/amino": "^0.32.2",
    "@cosmjs/proto-signing": "^0.32.2",
    "@ethersproject/providers": "5.7.2",
    "compare-versions": "^6.1.0",
    "ethers": "5.7.2",
    "long": "^5.2.3"
  },
  "devDependencies": {
    "cosmjs-types": "^0.9.0",
    "tsc-alias": "^1.8.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
